model_name    1e5_sfe001_n1e4_BE14
mCloud    1e5
sfe       0.01
nCore     1e4
rCore     1


# Bonnor-Ebert sphere
dens_profile    densBE
densBE_Omega    14.1

# ISM properties
nISM    0.1
stop_n_diss 0.1
stop_t_diss 1

# Allow expansion beyond cloud
expansionBeyondCloud    True

# Output directory
path2output    outputs/density_profile_sweep

# Use adaptive solver
use_adaptive_solver True

# Run settings
stop_r    None
stop_t    5

# Suppress warnings to save memory
log_level    WARNING
